{
  "name": "server",
  "version": "1.0.0",
  "main": "app.js",
  "bin": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "create-test-doctor": "node create-test-doctor.js",
    "delete-test-doctor": "node delete-doctor.js",
    "build": "pkg . --out-path ../dist",
    "build:win": "npm rebuild better-sqlite3 --target=18.12.0 --arch=x64 && pkg . --targets node18-win-x64 --out-path ../dist && npm rebuild better-sqlite3",
    "build:all": "npm rebuild better-sqlite3 --target=18.12.0 --arch=x64 && pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path ../dist && npm rebuild better-sqlite3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "pkg": {
    "assets": [
      "../client/**/*",
      "../Doc Dizon/**/*",
      "node_modules/better-sqlite3/**/*"
    ],
    "scripts": [
      "routes/**/*.js",
      "middleware/**/*.js",
      "services/**/*.js",
      "utils/**/*.js",
      "websocket/**/*.js",
      "data/db.js"
    ],
    "outputPath": "../dist",
    "targets": [
      "node18-win-x64"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.10",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}